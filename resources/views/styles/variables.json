{
    "spacer": "12px",
    "blue": "hsl(205, 82%, 57%)",
    "blue-hover": "hsl(205, 82%, 47%)",
    "cyan-lighter": "hsl(189, 46%, 92%)",
    "cyan-light": "hsl(190, 73%, 80%)",
    "cyan": "hsl(190, 73%, 59%)",
    "cyan-hover": "hsl(190, 73%, 49%)",
    "cyan-dark": "hsl(193, 100%, 34%)",
    "green-dark": "#288f18",
    "green": "hsl(105, 52%, 58%)",
    "green-hover": "hsl(105, 52%, 48%)",
    "orange-light": "hsl(29, 100%, 82%)",
    "orange": "hsl(29, 100%, 62%)",
    "orange-hover": "hsl(29, 100%, 52%)",
    "orange-dark": "hsl(29, 100%, 48%)",
    "purple": "hsl(245, 51%, 68%)",
    "purple-dark": "hsl(245, 49%, 52%)",
    "red": "hsl(0, 79%, 66%)",
    "red-hover": "hsl(0, 79%, 56%)",
    "red-dark": "hsl(0, 56%, 54%)",
    "red-darker": "hsl(0, 53%, 45%)",
    "yellow-light": "hsl(45, 100%, 68%)",
    "yellow": "hsl(45, 100%, 48%)",
    "yellow-hover": "hsl(45, 100%, 38%)",
    "yellow-dark": "hsl(35, 100%, 32%)",
    "pink-light": "hsl(7, 100%, 96%)",
    "pink": "hsl(7, 72%, 89%)",
    "pink-dark": "hsl(7, 15%, 57%)",
    "white": "hsl(0, 0%, 100%)",
    "white-hover": "hsla(0, 0%, 100%, 0.9)",
    "gray-lighter": "hsl(204, 6%, 97%)",
    "gray-lighter-hover": "hsl(204, 6%, 72%)",
    "gray-light": "hsl(204, 6%, 88%)",
    "gray-light-hover": "hsl(204, 6%, 72%)",
    "gray": "hsl(204, 6%, 55%)",
    "gray-hover": "hsl(204, 6%, 40%)",
    "gray-dark": "hsl(204, 6%, 45%)",
    "gray-dark-hover": "hsl(204, 6%, 30%)",
    "gray-darker": "hsl(204, 6%, 25%)",
    "gray-darker-hover": "hsl(204, 6%, 20%)",
    "black": "hsl(0,0%,0%)",
    "padding-xxxs": "calc($spacer / 4)",
    "padding-xxs": "calc($spacer / 3)",
    "padding-xs": "calc($spacer / 2)",
    "padding-sm": "$spacer",
    "padding-md": "calc($spacer * 2)",
    "padding-lg": "calc($spacer * 3)",
    "padding-xl": "calc($spacer * 5)",
    "padding-xxl": "calc($spacer * 7)",
    "padding-xxxl": "calc($spacer * 16)",
    "width-xxs": "calc($spacer / 2)",
    "width-xs": "calc($spacer)",
    "width-sm": "calc($spacer * 2)",
    "width-md": "calc($spacer * 4)",
    "width-lg": "calc($spacer * 6)",
    "width-xl": "calc($spacer * 12)",
    "width-xxl": "calc($spacer * 15)",
    "width-xxxl": "calc($spacer * 24)",
    "width-xxxxl": "calc($spacer * 32)",
    "font-size-xs": "13px",
    "font-size-sm": "14px",
    "font-size-md": "16px",
    "font-size-lg": "19px",
    "font-size-xl": "21px",
    "font-size-xxl": "30px",
    "font-size-xxxl": "48px",
    "font-size-xxxxl": "64px",
    "line-height-xs": "100%",
    "line-height-sm": "120%",
    "line-height-md": "135%",
    "line-height-lg": "150%",
    "line-height-xl": "200%",
    "line-height-xxl": "220%",
    "font-text-xs": "$font-weight-normal $font-size-xs / $line-height-lg $font-family, sans-serif",
    "font-text-sm": "$font-weight-normal $font-size-sm / $line-height-lg $font-family, sans-serif",
    "font-text-md": "$font-weight-normal $font-size-md / $line-height-lg $font-family, sans-serif",
    "font-text-lg": "$font-weight-normal $font-size-lg / $line-height-lg $font-family, sans-serif",
    "font-heading-xs": "$font-weight-semibold $font-size-sm / $line-height-lg $font-family, sans-serif",
    "font-heading-sm": "$font-weight-semibold $font-size-md / $line-height-lg $font-family, sans-serif",
    "font-heading-md": "$font-weight-semibold $font-size-lg / $line-height-lg $font-family, sans-serif",
    "font-heading-lg": "$font-weight-semibold $font-size-xl / $line-height-md $font-family, sans-serif",
    "font-heading-xl": "$font-weight-bold $font-size-xxl / $line-height-sm $font-family, sans-serif",
    "font-heading-xxl": "$font-weight-bold $font-size-xxxl / $line-height-xs $font-family, sans-serif",
    "font-heading-xxxl": "$font-weight-bold $font-size-xxxxl / $line-height-xs $font-family, sans-serif",
    "font-code-sm": "$font-weight-normal $font-size-sm / $line-height-md $font-family-code, monospace",
    "font-code-md": "$font-weight-normal $font-size-md / $line-height-md $font-family-code, monospace",
    "transition-md": "all 0.2s"
}
